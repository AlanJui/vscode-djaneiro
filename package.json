{
    "name": "vscode-djaneiro",
    "displayName": "Djaneiro - Django Snippets",
    "description": "A collection of snippets for django templates, models, views, fields & forms. Ported from Djaneiro for SublimeText",
    "version": "1.1.5",
    "publisher": "thebarkman",
    "icon": "images/djaneiro.png",
    "galleryBanner": {
        "color": "#0C4B33",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "snippets",
        "python",
        "django"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ScottBarkman/vscode-djaneiro"
    },
    "contributes": {
        "languages": [
            {
                "id": "html",
                "aliases": [
                    "Django Template",
                    "HTML"
                ],
                "extensions": [".html", ".htm"],
                "configuration": "./django-html.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "html",
                "scopeName": "text.html.django",
                "path": "./syntaxes/django-html.json"
            }
        ],        
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/admin/fieldsets.json"
            },
            {
                "language": "python",
                "path": "./snippets/admin/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/admin/methods.json"
            },
            {
                "language": "python",
                "path": "./snippets/forms/classes.json"
            },
            {
                "language": "python",
                "path": "./snippets/forms/fields.json"
            },
            {
                "language": "python",
                "path": "./snippets/forms/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/models/classes.json"
            },
            {
                "language": "python",
                "path": "./snippets/models/fields.json"
            },
            {
                "language": "python",
                "path": "./snippets/models/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/models/methods.json"
            },
            {
                "language": "python",
                "path": "./snippets/python/classes.json"
            },
            {
                "language": "python",
                "path": "./snippets/python/python.json"
            },
            {
                "language": "python",
                "path": "./snippets/settings/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/views/views.json"
            },
            {
                "language": "python",
                "path": "./snippets/views/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/templates/methods.json"
            },
            {
                "language": "python",
                "path": "./snippets/templates/imports.json"
            },
            {
                "language": "html",
                "path": "./snippets/templates/html.json"
            },
            {
                "language": "python",
                "path": "./snippets/url/imports.json"
            },
            {
                "language": "python",
                "path": "./snippets/url/methods.json"
            },
            {
                "language": "python",
                "path": "./snippets/url/regexes.json"
            }
        ]
    }
}
